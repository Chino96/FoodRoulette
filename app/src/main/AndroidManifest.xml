<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.recyclerview">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="false"
        android:icon="@drawable/iconwheel"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconwheel"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:allowBackup">
        <activity android:name="com.example.foodroulette.SelectActivity" />
        <activity android:name="com.example.foodroulette.LoginActivity" />
        <activity android:name="com.example.foodroulette.HomeActivity" />
        <activity android:name="com.example.foodroulette.RouletteActivity" />
        <activity android:name="com.example.foodroulette.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>